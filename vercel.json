{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/electronic-data-capture"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "ng build --configuration=production",
  "outputDirectory": "dist/electronic-data-capture",
  "installCommand": "npm install",
  "framework": "angular"
}
